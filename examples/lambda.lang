/**
 * Lambda function declaration and call
 * 
 * PrimaryExpression
 * : ParenthesizedExpression LambdaTail
 *
 * LambdaTail
 * : ARROW BlockStatement
 */

// Lambda function declaration
let point3d = (x, y, z) ->
{
    return new Point3D(x, y, z);
};

// Lambda function call
let p = point3d(1, 1, 2);